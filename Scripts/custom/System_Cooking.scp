[TYPEDEF t_cask]

ON=@dclick
src.sysmessage What do you wish to fill?
TARGET
return 1

ON=@targon_item
IF (<src.targ.type> == t_potion_empty)
	NEWITEM <TDATA2>
	IF (<src.targ.cont>)
		NEW.CONT = <src.targ.cont>
	ELSE
		NEW.P = <src.targ.p>
	ENDIF
	TAG.SERV -= 1
	IF  (<src.targ.amount> >= 2)
		src.targ.amount -= 1
		src.targ.update
	ELSE
		src.targ.remove
	ENDIF
	IF (<TAG.SERV> == 0)
		NEWITEM <TDATA1>
		src.sysmessage The <name> is now empty.
		IF (<cont>)
			NEW.CONT = <cont>
		ELSE
			NEW.P = <p>
		ENDIF
		remove
	ENDIF
	return 1
ENDIF



