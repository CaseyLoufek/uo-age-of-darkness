[itemdef i_deed_grainmill_w]
id=i_deed
type=t_script
weight=100
value=600
name="deed for grainmill (W)"
RESOURCES=1 i_crate_lg, 2 i_millstone, 10 i_board, 18 i_log, 2 i_nails
SKILLMAKE=Tinkering 86.9
CATEGORY=Provisions - Deeds
SUBSECTION=Item Deeds
DESCRIPTION=grainmill (w)

on=@dclick
  targetg="where should the grain mill be placed??"
  return 1

on=@targon_ground
  serv.newitem 01920
  new.p=<src.targp>
  new.move w 1
  new.attr 010
  link=<new.uid>
  serv.newitem 01922
  new.p=<src.targp>
  new.attr=010
  new.link=<link>
  link.link=<new.uid>
  remove
  return 1

on=@targon_char
  serv.newitem 01920
  new.p=<src.targ.p>
  new.move w 1
  new.attr 010
  link=<new.uid>
  serv.newitem 01922
  new.p=<src.targ.p>
  new.attr=010
  new.link=<link>
  link.link=<new.uid>
  remove
  return 1

on=@targon_item
  serv.newitem 01920
  new.p=<src.targ.p>
  new.move w 1
  new.attr 010
  link=<new.uid>
  serv.newitem 01922
  new.p=<src.targ.p>
  new.attr=010
  new.link=<link>
  link.link=<new.uid>
  remove
  return 1

[ITEMDEF 01920]
DEFNAME=i_flour_mill
RESOURCES=1 i_deed_grainmill_w
weight=100
name="grainmill"
flip=0
dupelist=01921,01922,01925,01926,01927
CATEGORY=Items by Professions
SUBSECTION=Baker
DESCRIPTION=grainmill (anim)

on=@create
  more1l={2000 2500}
  more1h=<more1l>
  morez=0

on=@dclick
  if (<distance> > 2) && !(<src.isgm>)
    src.sysmessage="You are too far away."
  elseif (<morez> != 0)
    src.sysmessage="The mill is busy - wait until it's ready again."
    timer=3
    link.timer=3
  elseif (<more1l> < 1) && !((<src.isgm>) || (<attr> & attr_owned))
    src.sfx 0220
    src.sysmessage="The mill is used up and disappears in a cloud of dust!"
    serv.newitem i_debris
    new.p=<p>
    new.attr=attr_decay
    new.timer=15
    remove
  else
    targetg="What grain do you want to work on?"
  endif
  return 1

on=@targon_item
  if (<src.targ.topobj.uid> != <src.uid>)
    src.sysmessage="You must have it with you."
  elseif (<src.targ.baseid> == i_wheat_sheaf)
    if (<src.restest 2 i_wheat_sheaf>)
      src.consume 2 i_wheat_sheaf
      morex=1    // Wheat
      f_anim_muehle_w
      if !((<src.isgm>) || (<attr> & attr_owned))
        more1l -= 1
      endif
    else
      src.sysmessage="It's too less - you need at least two bushels."
    endif
  elseif (<src.targ.baseid> == i_fruit_corn)
    if (<src.restest 10 i_fruit_corn>)
      src.consume 10 i_fruit_corn
      morex=2    // corn
      f_anim_muehle_w
      if !((<src.isgm>) || (<attr> & attr_owned))
        more1l -= 1
      endif
    else
      src.sysmessage="It's too less - you need at least 10 corn cobs."
    endif
  else
    src.sysmessage="You cannot use this with this mill."
  endif
  return 1

on=@timer
  if (<morez> == 1)
    if (<dispid> == 01921)    // back part full
      dispid=01925        // back part half full
      // link.dispid  front part left unchanged at 01926 - full
    elseif ((<dispid> == 01926) || (<dispid> == 01927))
      // dispid front part left unchanged
      link.dispid=01925        // back part half full
    endif
    link.sfx 05a9
    link.sfx 05ab
    morez=2
    link.morez=2
    timer=5
  elseif (<morez> == 2)
    if (<dispid> == 01925)    // back part half full
      dispid=01920        // back part empty (i_flour_mill)
      link.dispid=01922        // front part empty
    elseif ((<dispid> == 01926) || (<dispid> == 01927))    // front part full voll (Anim)
      dispid=01922        // front part empty
      link.dispid=01920        // back part empty (i_flour_mill)
    endif
    morez=0
    link.morez=0
  endif
  update
  link.update
  if (<morex> == 1)
    serv.newitem i_flour_sack
    new.p=<p>
    new.move e 2
  elseif (<morex> == 2)
    serv.newitem i_flour_sack
    new.p=<p>
    new.move e 2
  endif
  morex=0
  return 1

[function f_anim_muehle_w]
  if ((<dispid> == 01920) || (<dispid> == i_flour_mill))
    dispid=01921
    link.dispid=01926
  else    // 01922
    dispid=01926
    link.dispid=01921
  endif
  link.sfx 05a9
  link.sfx 05ab
  morez=1
  link.morez=1
  update
  link.update
  timer=5

