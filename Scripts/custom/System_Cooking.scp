[typedef t_edible]

ON=@DCLICK
IF !(STRMATCH(*<TYPE>*,"<SRC.FOODTYPE>")) && !(STRMATCH(*<DEFNAME>*,"<SRC.FOODTYPE>"))
	SRC.sysMessage You can't really consume this.
	RETURN 1
ENDIF
//IF (<src.flags> & statf_war)
//	SRC.sysMessage You can't consume that while fighting.
//	RETURN 1
//ENDIF
IF (<SRC.FOOD> < <SRC.MAXFOOD>)
	SRC.hits += <TAG0.HITS>
	SRC.stamina += <TAG0.STAM>
	SRC.mana += <TAG0.MANA>
	IF (<SRC.mana> > <SRC.MAXmana>)
		SRC.mana=<SRC.MAXmana>
	ENDIF
	IF (<SRC.hits> > <SRC.MAXhits>)
		SRC.hits=<SRC.MAXhits>
	ENDIF
	IF (<SRC.stam> > <SRC.MAXstam>)
		SRC.stam=<SRC.MAXstam>
	ENDIF

	IF !<SRC.RESTEST I_food_timer>
	      SRC.newitem i_food_timer
	      src.act.tag.hits=<TAG0.HITS>
	      src.act.tag.stam=<TAG0.STAM>
	      src.act.tag.mana=<TAG0.MANA>
	      SRC.act.equip 
	ELSE
	      OBJ=<SRc.findid.i_food_timer>
	      obj.tag0.hits += <TAG0.HITS>
	      obj.tag0.stam += <TAG0.STAM>
	      obj.tag0.mana += <TAG0.MANA>
	ENDIF

	IF !(<MOREM> == 0)
		SRC.FOOD += <MOREM>
	ELSE
		SRC.FOOD += (<WEIGHT>/10)
	ENDIF
	OBJ = <UID>
	IF  (<obj.amount> >= 2)
		obj.amount -= 1
		obj.dupe
		new.amount = 1
		iF (<obj.cont>)
			NEW.CONT = <obj.cont>
		ELSE
			NEW.P = <obj.p>
		ENDIF
		obj.update
		obj = <new>
		obj.update
	ENDIF
	IF (<OBJ.TAG0.SERV> > 1)
		OBJ.TAG.SERV -= 1
		SRC.emote have some of a <obj.name>
	ELSE
		IF !(<OBJ.TYPE> == t_food_raw) && !(<OBJ.TYPE> == t_meat_raw) && !(<OBJ.TYPE> == t_fruit)
			IF (<OBJ.TDATA1>)
				SRC.NEWITEM (<OBJ.TDATA1>)
				SRC.ACT.BOUNCE
			ENDIF
		ENDIF
		obj.REMOVE
		SRC.emote finish a <OBJ.name>
	ENDIF
	SRC.HUNGRY
	RETURN 1
ELSE
	SRC.SYSMESSAGE You are simply too full to eat anymore!
	RETURN 1
ENDIF



[itemdef i_food_timer]
Id=i_memory
name=Food
TYPE=t_eq_script

on=@create
ATTR=attr_invis|attr_decay

On=@Equip
   link = <src.uid>
   timer=2
   return

on=@timer
IF (<LINK.MAXHITS> > <LINK.HITS>) && <TAG0.HITS>) 
	LINK.HITS += 1
	TAG0.HITS -= 1
ENDIF
IF (<LINK.MAXMANA> > <LINK.MANA>) && <TAG0.MANA>) 
	LINK.MANA += 1
	TAG0.MANA -= 1
ENDIF
IF (<LINK.MAXSTAM> > <LINK.STAM>) && <TAG0.STAM>) 
	LINK.STAM += 1
	TAG0.STAM -= 1
ENDIF

IF (!<TAG0.STAM> && !<TAG0.HITS> && !<TAG0.MANA>)
	remove
ELSE
	timer=2
ENDIF
return 1

[function f_scavenge]
FORITEMS 3
	IF (<TYPE>==T_CORPSE)
		trigger=@cutoff
	RETURN
ENDFOR

[function f_cutoff]
if (<more1> == 0)
  src.sysmessage="It has already been gutted."
  return 1
endif
if <distance> > 4
  src.sysmessage="You are too far away"
  return 1
else
  local.ptr = 1
  while <serv.chardef(<amount>).resources.<dlocal.ptr>.val>
    for local.x 1 <serv.chardef(<amount>).resources.<dlocal.ptr>.val>
      serv.newitem <serv.chardef(<amount>).resources.<dlocal.ptr>.key>
	new.cont=<uid>
	NEW.USEITEM
    endfor
    local.ptr += 1
  endwhile
  local.ptr=
  local.x=
  more1=0
  return 1
endif 

