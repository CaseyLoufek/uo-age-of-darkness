[ITEMDEF i_commodity_art]
DEFNAME i_art_crate
ID i_crate_small_2
NAME Crate of Artwork
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
