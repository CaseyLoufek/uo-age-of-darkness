RETURN 1

[ITEMDEF i_commodity_pigments]
DEFNAME i_pigment_crate
ID i_crate_small_2
NAME Crate of Pigment
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
