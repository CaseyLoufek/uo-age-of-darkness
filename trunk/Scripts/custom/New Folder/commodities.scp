RETURN 1

[ITEMDEF i_commodity_glass]
DEFNAME i_glass_crate
ID i_crate_small_2
NAME Crate of Glass
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
