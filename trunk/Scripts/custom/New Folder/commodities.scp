RETURN 1

[ITEMDEF i_commodity_wine]
DEFNAME i_wine_crate
ID i_crate_small_2
NAME Crate of Wine Bottles
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
