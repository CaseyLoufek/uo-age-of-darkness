RETURN 1

[ITEMDEF i_commodity_tools]
DEFNAME i_tool_crate
ID i_crate_small_2
NAME Crate of Tools
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
