RETURN 1

[ITEMDEF i_commodity_parchment]
DEFNAME i_parchment_crate
ID i_crate_small_2
NAME Crate of Parchment
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
