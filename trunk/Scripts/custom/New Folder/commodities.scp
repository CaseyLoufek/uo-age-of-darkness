RETURN 1

[ITEMDEF i_commodity_shrinks]
DEFNAME i_shrink_crate
ID i_crate_small_2
NAME Crate of Shrunken Animals
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
