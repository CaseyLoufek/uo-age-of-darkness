RETURN 1

[ITEMDEF i_commodity_necro]
DEFNAME i_necro_crate
ID i_crate_small_2
NAME Crate of Necromantica
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

ON=@DCLICK
