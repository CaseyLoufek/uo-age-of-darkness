RETURN 1

[ITEMDEF i_commodity_medical]
DEFNAME i_medical_crate
ID i_crate_small_2
NAME Crate of Medical Supplies
TYPE t_normal
WEIGHT 100

ON=@CREATE
ATTR=attr_decay

