//
//For the delay to work, we need to give the player
//a t_eq_script item that will remove itself at the
//timer.
[ITEMDEF i_commoditywait_timer]
NAME CommodityWait Timer
TYPE t_eq_script
ITEMDEF i_commoditywait_timer
ID=i_gold

ON=@CREATE
ATTR attr_decay|attr_invis
TIMER=6
RETURN 1

ON=@TIMER
LINK.TAG.COMMODITYWAIT=
TIMER=-1
REMOVE
RETURN 1


