ON=@EquipTest
	if ( <src.str> < <EVAL (<tag0.REQSTR>+((<tag0.REQSTR>*<tag0.lowerreqs>)/100))> )
		src.sysmessage="You are not strong enough to equip that."
		return 1
	endif
	IF (<attr>&attr_magic) && !(<attr>&attr_identified)
		DORAND 5
			SRC.SPELLEFFECT S_WEAKEN 1000
			SRC.SPELLEFFECT S_FEEBLEMIND 1000
			SRC.SPELLEFFECT S_CLUMSY 1000
			SRC.SPELLEFFECT S_MAGIC_ARROW 1000
			SRC.SPELLEFFECT S_HARM 1000
		ENDDO
		SRC.SYSMESSAGE Due to your lack of understanding the unidentified magics of the item backfire upon you.
		RETURN 1
	ENDIF

