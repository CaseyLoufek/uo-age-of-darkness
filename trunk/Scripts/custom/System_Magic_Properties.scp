ON=@Timer
	IF (<CONT.ISPLAYER>)
		IF (<TAG0.MANAREGEN>)
			CONT.MANA += (<TAG0.MANAREGEN>)
			IF (<CONT.MANA> > <CONT.MAXMANA>)
				CONT.MANA=<CONT.MAXMANA>
			endif
			TIMER=10
		ENDIF
		IF (<TAG0.HITSREGEN>)
	 		CONT.HITS += (<TAG0.HITSREGEN>)
			IF (<CONT.HITS> > <CONT.MAXHITS>)
				CONT.HITS=<CONT.MAXHITS>
			endif
			TIMER=10
		ENDIF
		IF (<TAG0.STAMREGEN>)
	 		CONT.STAM += (<TAG0.STAMREGEN>)
			IF (<CONT.STAM> > <CONT.MAXSTAM>)
				CONT.STAM=<CONT.MAXSTAM>
			endif
			TIMER=10
		ENDIF
		IF (<TAG0.SELFREPAIR>)
	 		HITS += (Rand(<eval <TAG0.SELFREPAIR>+1>))
			IF (<HITS> > <MAXHITS>)
				HITS=<MAXHITS>
			endif
			UPDATE
			TIMER=10
		ENDIF
	RETURN 1
	endif



