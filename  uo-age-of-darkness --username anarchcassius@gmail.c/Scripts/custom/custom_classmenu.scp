[ITEMDEF i_cards_of_destiny]
ID=012a8
DEFNAME=i_CARDS_DESTINY
NAME=Cards of Destiny

CATEGORY=Special Items
SUBSECTION=Character Select
DESCRIPTION=Cards Of Destiny

ON=@CREATE
TYPE=1

ON=@DCLICK
IF (STRMATCH(*<SRC.TAG0.RACE>*,"Orc"))
	DIALOG D_ORC_CLASS
	RETURN 1
ENDIF
If (<SRC.BODY> == c_man) ||(<SRC.BODY> == c_woman)
		DIALOG D_HUMAN_CLASS
		RETURN 1
ENDIF
If (<SRC.BODY> == c_elf_male) || (<SRC.BODY> == c_elf_female)
		DIALOG D_ELF_CLASS
		RETURN 1
ENDIF



[function race_human]
src.events +e_environmental
SRC.MAXFOLLOWER=6
SRC.EVENTS +e_Level_up
SRC.EVENTS +e_tailoring_color
SRC.EVENTS +e_WeaponSkills
SRC.givelang 1 //This will give them the english language.
SRC.TAG0.MODMAXWEIGHT += 25
SRC.MAXHITS=(<SRC.STR>)
SRC.MAXMANA=(<SRC.INT>)
SRC.MAXSTAM=(<SRC.DEX>)
If (<SRC.BODY> == c_man) // human males are stronger and tougher
SRC.STR=(<SRc.STR>+4)
SRC.HITS=(<SRc.HITS>+4)
SRC.MAXHITS=(<SRc.MAXHITS>+4)
SRC.TAG0.OVERRIDE.STATCAP_0=(<SRC.SKILLCLASS.STR>+4)
endif
If (<SRC.BODY> == c_woman) // human females are more agile and enduring
SRC.DEX=(<SRc.DEX>+4)
SRC.STAMINA=(<SRc.STAMINA>+4)
SRC.MAXSTAM=(<SRc.MAXSTAM>+4)
SRC.TAG0.OVERRIDE.STATCAP_2=(<SRC.SKILLCLASS.DEX>+4)
ENDIF
SRC.HITS=(<SRC.HITS>+8) // humans are resilant
SRC.TAG0.MODHITS += 8
//as part of classes humans have greater skill cap
SRC.TAG0.RACE=Human
RETURN 0

[function race_elf]
src.events +e_environmental
SRC.MAXFOLLOWER=6
SRC.EVENTS +e_Level_up
SRC.EVENTS +e_tailoring_color
SRC.EVENTS +e_WeaponSkills
SRC.givelang 2 //This will give them the elven language.
SRC.givealtlang_1 1 //This will give them the english language.
SRC.DEX=(<SRc.DEX>+4) //elves are quick
SRC.MAXHITS=(<SRC.STR>)
SRC.MAXMANA=(<SRC.INT>)
SRC.MAXSTAM=(<SRC.DEX>)
SRC.MANA=(<SRc.MANA>+8) // elves are innately magical
SRC.TAG0.MODMANA += 8
SRC.RESENERGY=10 // elves are resistant to natural and magical energy
SRC.NIGHTSIGHT=1 // elves have permanant nightsight
SRC.TAG0.RACE=Elf
RETURN 0

[function convert_orc]
If (<SRC.BODY> == c_elf_male)
	src.body=c_man
	src.obody=c_man
endif
If (<SRC.BODY> == c_elf_female)
	src.body=c_woman
	src.obody=c_woman
endif
src.color=<ARGV[0]>
src.oskin=<ARGV[0]>
SRC.FINDLAYER.11.REMOVE
SRC.FINDLAYER.16.REMOVE
NEWITEM i_head_orc
src.EQUIP <NEW>
SRC.TAG0.RACE=Orc
return

[function race_orc]
src.events +e_environmental
SRC.MAXFOLLOWER=6
SRC.EVENTS +e_Level_up
SRC.EVENTS +e_tailoring_color
SRC.EVENTS +e_WeaponSkills
SRC.givelang 4 //This will give them the orcish language.
SRC.str=(<SRc.str>+4) //orcs are strong
If (<SRC.BODY> == c_man) // orc males are stronger and tougher
SRC.STR=(<SRc.STR>+4)
SRC.HITS=(<SRc.HITS>+4)
SRC.MAXHITS=(<SRc.MAXHITS>+4)
SRC.TAG0.OVERRIDE.STATCAP_0=(<SRC.SKILLCLASS.STR>+4)
endif
If (<SRC.BODY> == c_woman) // orc females are more agile and enduring
SRC.DEX=(<SRc.DEX>+4)
SRC.STAMINA=(<SRc.STAMINA>+4)
SRC.MAXSTAM=(<SRc.MAXSTAM>+4)
SRC.TAG0.OVERRIDE.STATCAP_2=(<SRC.SKILLCLASS.DEX>+4)
ENDIF
SRC.MAXHITS=(<SRC.STR>)
SRC.MAXMANA=(<SRC.INT>)
SRC.MAXSTAM=(<SRC.DEX>)
SRC.STAM=(<SRc.STAM>+8) // orcs are tireless
SRC.TAG0.MODSTAM += 8
SRC.RESPOISON=10 // orcs are resistant to toxiins
SRC.TAG0.RACE=Orc
RETURN 0


